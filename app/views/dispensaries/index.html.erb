<h1>Listing dispensaries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dispensaries.each do |dispensary| %>
      <tr>
        <td><%= dispensary.name %></td>
        <td><%= dispensary.city %></td>
        <td><%= dispensary.state %></td>
        <td><%= link_to 'Show', dispensary %></td>
        <td><%= link_to 'Edit', edit_dispensary_path(dispensary) %></td>
        <td><%= link_to 'Destroy', dispensary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dispensary', new_dispensary_path %>
